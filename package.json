{
  "name": "tanstack-start",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "./dev.sh",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "npx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
